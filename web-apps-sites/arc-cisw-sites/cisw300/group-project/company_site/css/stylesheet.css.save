@import url('https://fonts.googleapis.com/css?family=Audiowide');

#Wrapper {
	height: 95%;
	width: 100%;
	margin: 0;
	
}

#Header {
	height: 10vh;
	width: 100%;
	float: left;

	
}

#Content {
	height: 90%;
	width: 100%;
	padding-top: 20px;
	padding-bottom: 140px;
	overflow: auto;
}

#prime {
	transform:translateY(15px);
	
	
}

header {
	position:fixed;
	top: 0;
	left: 0;
	background-image: url("../images/nav998metal.jpg");
	background-repeat: no-repeat;
	background-position: top;
	background-size: cover;
	width: 100vw;
	min-height:125px;
	z-index:1;
	font-family: Audiowide, Cursive;
}

nav {
	margin-top: 59px;
	float: right;
	position: fixed;
	z-index:2;
	margin-left: 25%;
	width: 100%;
	font-family: Audiowide, Cursive;
}
main {
	top: 84px;
	width: 100%;
	height: 260px;
	padding-top: 120px;
	padding-bottom: 140px;
	/*overflow-y: auto;*/
	
}

nav a{
	text-decoration: none;
	border: outset 3px grey;
	
}

footer a{
	text-decoration: none;
	color: #075341;
	border: solid 3px black;
}

h1	{
	font-family: 'Open Sans', sans-serif;
	font-size: 25px;
}

h2 {
	font-size: 22px;
}
a:link {
		border: outset 3px grey;
		text-shadow: 1px 1px #000000; 
		color: #FA8752;
		padding: 5px;
		box-sizing: border-box;
		}

a:visited { Color: #CD1931;}

a:hover {
		border: inset 3px grey;
		color: #07DB41;
		
background-color:rgba(255,255,255,0.3);
		}

footer ul { list-style-type: none;}

nav ul { list-style-type: none;}

footer li { display: inline;}

nav li { display: inline;}

	p {	font-family: 'Open Sans condensed', sans-serif; 
		font-size: 18px;
		padding-left: 10%;}

div.footer {
	margin-top: 140px;
	position: fixed;
	z-index:2;
	color: white;
	font-family: 'Cormorant Garamond', serif;
	font-size: 14px;
	text-align: left;
}

footer {
	position:fixed;
	bottom:-20px;
	left:0px;
	font-size:.60em;
	font-style: italic;
	border-style:none;
	border-width: 0;
	margin-left:0;
	margin-right:0;
	background-image: url("../images/dash998.png");
	background-repeat: no-repeat;
	background-position: bottom;
	background-size: cover;
	width:100%;
	height:225px;
	min-height:176px;

	}



	
